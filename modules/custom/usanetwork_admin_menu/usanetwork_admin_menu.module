<?php

include_once 'usanetwork_admin_menu.features.inc';

/**
 * Implements hook_menu_alter().
 */
function usanetwork_admin_menu_menu_alter(&$items) {
  $items['admin_menu/toggle-modules'] = array(
    'page callback' => 'drupal_get_form',
    'page arguments' => array('usanetwork_admin_menu_toggle_modules'),
    'access arguments' => array('administer modules'),
    'type' => MENU_CALLBACK,
    'module' => 'usanetwork_admin_menu',
    'file' => 'usanetwork_admin_menu.inc',
  );
}
