<?php

/**
 * @file
 * Bulk export of strongarm objects generated by Bulk export module.
 */

/**
 * Implements hook_strongarm().
 */
function usanetwork_ds_strongarm() {
  $strongarms = array();

  $strongarm = new stdClass();
  $strongarm->disabled = FALSE; /* Edit this to true to make a default strongarm disabled initially */
  $strongarm->api_version = 1;
  $strongarm->name = 'field_bundle_settings_file__image';
  $strongarm->value = array(
    'view_modes' => array(
      'block_cover_title' => array(
        'custom_settings' => TRUE,
      ),
    ),
    'extra_fields' => array(
      'display' => array(
        'file' => array(
          'block_cover_title' => array(
            'weight' => '0',
            'visible' => TRUE,
          ),
        ),
      ),
    ),
  );
  $strongarms['field_bundle_settings_file__image'] = $strongarm;

  $strongarm = new stdClass();
  $strongarm->disabled = FALSE; /* Edit this to true to make a default strongarm disabled initially */
  $strongarm->api_version = 1;
  $strongarm->name = 'field_bundle_settings_node__media_gallery';
  $strongarm->value = array(
    'view_modes' => array(
      'block_cover_title' => array(
        'custom_settings' => TRUE,
      ),
    ),
    'extra_fields' => array(),
  );
  $strongarms['field_bundle_settings_node__media_gallery'] = $strongarm;

  return $strongarms;
}
