/**
 * @file
 * MPX video file entity additional usanetwork-specific configuration.
 */

/**
 * Implements hook_file_default_types_alter().
 */
function usanetwork_mpx_video_file_default_types_alter(&$types) {
  // Create field bases.
  $usa_mpx_field_bases = _usa_mpx_get_mpx_video_file_field_bases();
  foreach ($usa_mpx_field_bases as $field_name => $field_data) {
    if (!field_info_field($field_name)) {
      try {
        field_create_field($field_data);
      }
      catch (Exception $e) {
        watchdog('usanetwork_mpx_video', 'Failed to create field base "!field_name", Exception: !e', array(
          '!field_name' => $field_name,
          '!e' => $e->getTraceAsString()
        ), WATCHDOG_ERROR);

        continue;
      }
      field_info_field($field_name);
    }
  }

  // Create field instances.
  $video_file_types = _pub_mpx_get_mpx_account_video_file_types();
  $usa_mpx_field_instances = _usa_mpx_get_mpx_video_file_field_instances();

  foreach ($video_file_types as $bundle_name => $file_type) {
    foreach ($usa_mpx_field_instances as $field_name => $instance_data) {
      if (!field_info_instance('file', $field_name, $bundle_name)) {
        $instance_data['bundle'] = $bundle_name;
        try {
          field_create_instance($instance_data);
        }
        catch (Exception $e) {
          watchdog('usanetwork_mpx_video', 'Failed to create field instance "!field_name", Exception: !e', array(
            '!field_name' => $field_name,
            '!e' => $e->getTraceAsString()
          ), WATCHDOG_ERROR);

          continue;
        }
      }
    }
  }
}

